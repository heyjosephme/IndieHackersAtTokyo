---
import Navbar from "../components/Navbar.astro";
import Hero from "../components/Hero.astro";
interface Props {
    title: string;
}
const { title } = Astro.props;
---

<html lang="en">
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width" />
        <title>{title}</title>
    </head>
    <body>
        <Navbar />
        <main>
            <Hero nextEventDate="March 15" nextEventTime="19:00 - 21:00 JST" />
            <slot />
        </main>
    </body>
</html>
